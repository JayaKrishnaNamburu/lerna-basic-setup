{
  "packages": [
    "src/packages/*"
  ],
  "npmClient": "yarn",
  "npmClientArgs": [
    "--no-lockfile"
  ],
  "version": "0.1.8",
  "useWorkspaces": true,
  "command": {
    "publish": {
      "ignoreChanges": [
        "ignored-file",
        "*.md"
      ],
      "message": "chore(release): publish"
    }
  }
}
